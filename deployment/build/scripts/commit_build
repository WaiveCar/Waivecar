#!/bin/bash

# Exit at first failure
set -e
echo "`date -u `: Starting commit_build"

if [ "${TRAVIS_BRANCH}" == "web/development" ]; then
  export NODE_ENV=development
  npm run build
elif [ "${TRAVIS_BRANCH}" == "web/staging" ]; then
  export NODE_ENV=staging
  npm run build
elif [ "${TRAVIS_BRANCH}" == "web/master" ]; then
  export NODE_ENV=production
  npm run build
fi

echo "`date -u `: commit_build done"
