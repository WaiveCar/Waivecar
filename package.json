{
  "name": "waivecar",
  "version": "1.0.0",
  "description": "WaiveCar Mobile Application",
  "scripts": {
    "install": "napa"
  },
  "napa": {
    "ionic": "driftyco/ionic-bower#v1.0.0"
  },
  "browser": {
    "ionic": "./www/lib/ionic/js/ionic.js",
    "angular": "./node_modules/angular/angular.js",
    "angular-resource": "./node_modules/angular-resource/angular-resource.js",
    "angular-sanitize": "./node_modules/angular-sanitize/angular-sanitize.js",
    "ionic-angular": "./www/lib/ionic/js/ionic-angular.js",
    "angular-ui-router": "./node_modules/angular-ui-router/release/angular-ui-router.js",
    "angular-animate": "./node_modules/angular-animate/angular-animate.js",
    "angular-messages": "./node_modules/angular-messages/angular-messages.js",
    "ngCordova": "./node_modules/ng-cordova/dist/ng-cordova.js",
    "angular-socket-io": "./node_modules/angular-socket-io/socket.js",
    "angular-payments": "./node_modules/angular-payments/lib/angular-payments.js",
    "lodash": "./node_modules/lodash/index.js",
    "async": "./node_modules/async/lib/async.js",
    "socket.io-client": "./node_modules/socket.io-client/lib/index.js",
    "moment": "./node_modules/moment/moment.js",
    "sprintf": "./node_modules/sprintf-js/dist/sprintf.min.js",
    "jquery": "./node_modules/jquery/dist/jquery.js",
    "leaflet": "./www/vendors/leaflet/leaflet-src.js",
    "skobbler": "./www/vendors/skobbler/skobbler-2.0.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "angular": {
      "exports": "angular",
      "depends": [
        "jquery:jQuery"
      ]
    },
    "ionic": "ionic",
    "angular-animate": {
      "depends": [
        "angular"
      ]
    },
    "angular-resource": {
      "depends": [
        "angular"
      ]
    },
    "angular-ui-router": {
      "depends": [
        "angular"
      ]
    },
    "angular-sanitize": {
      "depends": [
        "angular"
      ]
    },
    "angular-socket-io": {
      "depends": [
        "angular"
      ]
    },
    "angular-payments": {
      "depends": [
        "angular"
      ]
    },
    "ionic-angular": {
      "depends": [
        "angular",
        "angular-animate",
        "angular-resource",
        "angular-sanitize",
        "angular-ui-router",
        "ionic"
      ]
    }
  },
  "dependencies": {},
  "devDependencies": {
    "angular-animate": "^1.4.5",
    "angular-messages": "^1.4.5",
    "angular-payments": "^1.0.7",
    "angular-resource": "^1.4.5",
    "angular-sanitize": "^1.4.5",
    "angular-socket-io": "^0.7.0",
    "angular-ui-router": "^0.2.15",
    "async": "^1.4.2",
    "bower": "^1.3.3",
    "browserify": "^11.1.0",
    "browserify-shim": "^3.8.10",
    "debowerify": "^1.3.1",
    "gulp": "^3.5.6",
    "gulp-concat": "^2.2.0",
    "gulp-eslint": "^1.0.0",
    "gulp-jshint": "^1.11.2",
    "gulp-minify-css": "^0.3.0",
    "gulp-ng-templates": "0.0.6",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "2.0.4",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-util": "^3.0.6",
    "jasmine-core": "^2.3.4",
    "jquery": "^2.1.4",
    "napa": "^1.2.0",
    "ng-cordova": "^0.1.20-alpha",
    "shelljs": "^0.3.0",
    "socket.io-client": "^1.3.6",
    "sprintf-js": "^1.0.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.4.0",
    "when": "^3.7.3"
  },
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-splashscreen",
    "com.ionic.keyboard",
    {
      "locator": "https://github.com/EddyVerbruggen/LaunchMyApp-PhoneGap-Plugin.git",
      "id": "nl.x-services.plugins.launchmyapp",
      "variables": {
        "URL_SCHEME": "WaiveCar"
      }
    },
    "cordova-plugin-whitelist",
    "cordova-plugin-geolocation",
    "cordova-plugin-camera",
    "cordova-plugin-file-transfer",
    "org.apache.cordova.inappbrowser"
  ],
  "cordovaPlatforms": [
    "ios",
    "android"
  ]
}
