{
  "name": "reach-api",
  "private": true,
  "scripts": {
    "prepublish": "npm prune",
    "test": "NODE_ENV=test node --harmony tests.js",
    "test-cov": "NODE_ENV=test node --harmony node_modules/.bin/istanbul cover tests.js",
    "test-travis": "mkdir -p ./coverage; (NODE_ENV=test istanbul cover node_modules/mocha/bin/_mocha -- --reporter spec --check-leaks --require test/support/should --recursive test/; echo $? > retval) | tee coverage/coverage.txt; egrep '^0$' retval",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "local": "NODE_ENV=local DEBUG=socket:* nodemon --harmony run.js",
    "dev": "NODE_ENV=dev node --harmony run.js",
    "stag": "NODE_ENV=stag node --harmony run.js",
    "prod": "NODE_ENV=prod node --harmony run.js",
    "start": "node --harmony run.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reach/api"
  },
  "author": "Christoffer RÃ¸dvik",
  "license": "MIT",
  "dependencies": {
    "change-case": "2.3.0",
    "co": "4.5.4",
    "co-bcrypt": "1.0.0",
    "co-fs": "1.2.0",
    "co-request": "0.2.1",
    "koa-cors": "0.0.16",
    "lodash": "3.10.0",
    "moment": "2.10.2",
    "reach-api": "0.2.x",
    "reach-auth": "0.0.6"
  },
  "devDependencies": {
    "chai": "2.3.0",
    "coveralls": "2.11.2",
    "istanbul-harmony": "0.3.x",
    "node-random-name": "1.0.1",
    "request": "2.58.0"
  }
}
