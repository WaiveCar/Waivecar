{
  "name": "waivecar",
  "private": true,
  "scripts": {
    "prepublish": "npm prune",
    "test": "NODE_ENV=test node --harmony tests.js",
    "test-cov": "NODE_ENV=test node --harmony node_modules/istanbul-harmony/lib/cli.js cover --hook-run-in-context tests.js",
    "test-travis": "mkdir -p ./coverage; (NODE_ENV=test node --harmony node_modules/istanbul-harmony/lib/cli.js cover --hook-run-in-context tests.js; echo $? > retval) | tee coverage/coverage.txt; egrep '^0$' retval",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "local": "NODE_ENV=local nodemon --harmony --no-crankshaft app.js",
    "start": "node --harmony --no-crankshaft app.js"
  },
  "dependencies": {
    "babel-eslint": "3.1.29",
    "change-case": "2.3.0",
    "co": "4.5.4",
    "co-bcrypt": "1.0.0",
    "co-fs": "1.2.0",
    "co-glob": "0.0.2",
    "co-request": "0.2.1",
    "eslint": "0.24.1",
    "extend": "3.0.0",
    "koa-cors": "0.0.16",
    "lodash": "3.10.0",
    "moment": "2.10.2",
    "reach-api": "0.3.26",
    "shortid" : "2.2.2"
  },
  "devDependencies": {
    "chai": "2.3.0",
    "coveralls": "2.11.2",
    "istanbul-harmony": "0.3.x",
    "node-random-name": "1.0.1",
    "request": "2.58.0"
  },
  "reach": {
    "reach-auth": "0.1.4",
    "reach-commander": "0.0.3",
    "reach-email": "0.0.4",
    "reach-file": "0.2.0",
    "reach-log": "0.0.11",
    "reach-payments": "0.0.7",
    "reach-queue": "0.0.7",
    "reach-sequelize": "0.0.10",
    "reach-sms": "0.0.4",
    "reach-user": "0.1.9",
    "reach-ui": "0.0.1"
  }
}
